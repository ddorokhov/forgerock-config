{
  "name": "protect-all",
  "baseURI": "http://home.whitedaos.dns-dynamic.net",
  "condition": "${true}",

  "handler": {
    "type": "Chain",
    "config": {
      "filters": [
        {
          "name": "basicAuth",
          "type": "BasicAuthFilter",
          "config": {
            "realm": "Protected",
            "users": [
              {
                "username": "admin",
                "password": "admin123!"
              }
            ]
          }
        }
      ],
      "handler": {
        "type": "ReverseProxyHandler",
        "config": {
          "target": "http://infra.whitedaos.dns-dynamic.net"
        }
      }
    }
  }
}
